<?xml version="1.0" encoding="utf-8" ?>
<?codebehind transformation="ToShape.xslt"?>
<shape xmlns="urn:gold">
    <name>Transition</name>


    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
    ~
    ~ Definition
    ~
    ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <definition>
        <sequence prefix="T" format="T{1:D4}" />
    </definition>


    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
    ~
    ~ Functional properties
    ~
    ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <section name="Functional">
        <string id="Event" name="Event">
            <required for="Analysis" />
            <length max="30" />
        </string>

        <string id="Description" name="Description">
            <required for="Analysis" />
            <length max="200" />
        </string>

        <string id="Notes" name="Notes" multiline="true" />
    </section>


    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
    ~
    ~ Technical properties
    ~
    ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <section name="Technical">
        <string id="TechNotes" name="Notes" multiline="true" />
    </section>
</shape>
